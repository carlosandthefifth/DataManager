<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Object_API_Name</fullName>
    <active>true</active>
    <description>Object API Name can never be blank</description>
    <errorConditionFormula>IF( ISBLANK( Object_API_Name__c ) , true, false)</errorConditionFormula>
    <errorDisplayField>Object_API_Name__c</errorDisplayField>
    <errorMessage>Object API Name is required</errorMessage>
</ValidationRule>
